<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mrzk.example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mrzk.example.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mrzk.example.pagelist.PageDetailActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.mrzk.example.login.RegisterActivity"/>
        <activity android:name="com.mrzk.example.login.LoginSuccessActivity"/>
        <activity android:name="com.mrzk.example.pagelist.PageListActivity"/>
        <activity android:name="com.mrzk.example.login.LoginActivity"/>
        <activity android:name="com.mrzk.example.pagelist.PageDetail2Activity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.mrzk.example.pagelist.PageDetail3Activity"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>